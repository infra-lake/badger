{
  "name": "badger",
  "version": "1.0.6",
  "description": "Badger is a tool that export MongoDB data to Google BigQuery",
  "main": "src/index.mjs",
  "scripts": {
    "build": "rm -rf dist;tsc",
    "start": "make up;nodemon --watch src/ --ext ts,json --exec \"npm run build; node dist/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infra-lake/badger.git"
  },
  "keywords": [
    "mongodb",
    "bigquery"
  ],
  "author": "Herberton Candido Souza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/infra-lake/badger/issues"
  },
  "homepage": "https://github.com/infra-lake/badger#readme",
  "dependencies": {
    "@google-cloud/bigquery": "^6.2.1",
    "amqp-connection-manager": "^4.1.13",
    "amqplib": "^0.10.3",
    "bytes": "^3.1.2",
    "dotenv": "^16.1.4",
    "handlebars": "^4.7.7",
    "mongodb": "^5.6.0",
    "object-sizeof": "^2.6.3",
    "prom-client": "^14.2.0",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/amqp-connection-manager": "^3.4.1",
    "@types/amqplib": "^0.10.1",
    "@types/bytes": "^3.1.1",
    "@types/js-yaml": "^4.0.5",
    "@types/qs": "^6.9.7",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.1.3"
  },
  "jest": {
    "testMatch": [
      "**/*.spec.mjs"
    ]
  }
}
